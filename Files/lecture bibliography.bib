@article{andersen2019,
  title = {The Importance of Liking of Appearance, -Odour, -Taste and -Texture in the Evaluation of Overall Liking. {{A}} Comparison with the Evaluation of Sensory Satisfaction},
  author = {Andersen, Barbara Vad and Brockhoff, Per Bruun and Hyldig, Grethe},
