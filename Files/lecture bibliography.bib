@article{orden2019,
  title = {Testing {{SensoGraph}}, a Geometric Approach for Fast Sensory Evaluation},
  author = {Orden, David and {Fern{\'a}ndez-Fern{\'a}ndez}, Encarnaci{\'o}n and {Rodr{\'i}guez-Nogales}, Jos{\'e} M. and {Vila-Crespo}, Josefina},
