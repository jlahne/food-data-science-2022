@article{rousseau2015,
  title = {Sensory Discrimination Testing and Consumer Relevance},
  author = {Rousseau, Beno{\^i}t},
  year = {2015},
  month = jul,
  journal = {Food Quality and Preference},
  volume = {43},
  pages = {122--125},
  issn = {0950-3293},
  doi = {10.1016/j.foodqual.2015.03.001},
  abstract = {In order to ensure consistency in the decision making process over time, a discrimination testing program must take into account all of five elements: The testing protocol, the sample size, the Type I error ({$\alpha$}), the Type II error ({$\beta$}) (power=1-{$\beta$}) and a measure of the threshold above which the scientist has established that the difference is meaningful to the consumer ({$\delta$}R). Two putatively different products will always be found to be different provided that the sample size is large enough. This fact underscores the need to set {$\delta$}R. The concept of discriminators is attractive but flawed, as the same underlying sensory difference will result in different proportions of distinguishers depending on the method used. Prescott, Leslie, Kunst, and Kim (2005) proposed the idea of consumer rejection threshold which avoids the pitfalls of the proportion of discriminators concept. However, it is limited to differences that can be linked to a specific compound, such as one responsible for a product defect or off-flavor. In this manuscript two alternative approaches are discussed. The first one uses a special feature of the same-different protocol which permits the estimation of the size of the sensory difference above which consumers would call two products ``different''. The second one links the estimate of a standardized measure of sensory difference, d{${'}$}, to consumer hedonic response between the product pairs and finds the threshold above which a sensory difference results in a meaningful preference result. Experimental research is needed to study the suitability of these approaches. Ultimately, establishing {$\delta$}R is essential to ensure that results from a discrimination testing program are actually relevant to the consumers whose behavior it is trying to predict.},
  keywords = {Consumer relevance,Discrimination testing,Power,Sample size,Sensory difference},
  file = {/Users/jake/Dropbox/Work/Zotero PDF Files/Rousseau_2015_Sensory discrimination testing and consumer relevance.pdf;/Users/jake/Zotero/storage/CKWLN266/S0950329315000609.html}
}

