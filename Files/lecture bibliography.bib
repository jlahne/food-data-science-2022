@article{wilkes2000,
  title = {Sample Preparation for the Analysis of Flavors and Off-Flavors in Foods},
  author = {Wilkes, Jon G and Conte, Eric D and Kim, Yongkyoung and Holcomb, Manuel and Sutherland, John B and Miller, Dwight W},
  year = {2000},
