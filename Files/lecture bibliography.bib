@article{carpino2008,
  title = {Volatile Fingerprint of {{Piacentinu}} Cheese Produced with Different Tools and Type of Saffron},
  shorttitle = {Volatile Fingerprint of {{Piacentinu}} Cheese Produced with Different Tools and Type of Saffron},
