@article{gade2004,
  title = {Tradition, Territory, and Terroir in {{French}} Viniculture: {{Cassis}}, {{France}}, and Appellation Controlee},
  shorttitle = {Tradition, Territory, and Terroir in {{French}} Viniculture: {{Cassis}}, {{France}}, and Appellation Controlee},
