@article{vidal2022,
  title = {Biterm Topic Modelling of Responses to Open-Ended Questions: {{A}} Study with {{US}} Consumers about Vertical Farming},
  author = {Vidal, Leticia and Ares, Gast{\'o}n and Jaeger, Sara R.},
  year = {2022},
