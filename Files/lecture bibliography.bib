  author = {Quick, Virginia and {Byrd-Bredbenner}, Carol and White, Adrienne A. and Brown, Onikia and Colby, Sarah and Shoff, Suzanne and Lohse, Barbara and Horacek, Tanya and Kidd, Tanda and Greene, Geoffrey},
  year = {2014},
