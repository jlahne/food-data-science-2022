@article{galmarini2016,
  title = {Use of {{Multi-Intake Temporal Dominance}} of {{Sensations}} ({{TDS}}) to {{Evaluate}} the {{Influence}} of {{Cheese}} on {{Wine Perception}}: {{Influence}} of Cheese on Wine Perception\ldots},
