@article{bingman2020,
  title = {Monitoring {{Cider Aroma Development}} throughout the {{Fermentation Process}} by {{Headspace Solid Phase Microextraction}} ({{HS-SPME}}) {{Gas Chromatography}}\textendash{{Mass Spectrometry}} ({{GC-MS}}) {{Analysis}}},
  author = {Bingman, Matthew T. and Stellick, Claire E. and Pelkey, Jordanne P. and Scott, Jared M. and Cole, Callie A.},
  year = {2020},
  journal = {Beverages},
  volume = {6},
  number = {2},
  issn = {2306-5710},
  doi = {10.3390/beverages6020040},
  abstract = {Volatile organic compounds (VOCs) play a crucial role in cider quality. Many variables involved in the fermentation process contribute to cider fragrance, but their relative impact on the finished odor remains ambiguous, because there is little consensus on the most efficient method for cider volatile analysis. Herein, we have optimized and applied a headspace solid phase microextraction gas chromatography\&ndash;mass spectrometry (HS-SPME GC-MS) method for the chemical analysis of cider VOCs. We determined that the 30 min exposure of a divinylbenzene/carboxen/polydimethylsiloxane (DVB/CAR/PDMS) solid phase microextraction (SPME) fiber at 40 \&deg;C yielded detection of the widest variety of VOCs at an extraction efficiency \&gt;49\% higher than comparable fibers. As a proof-of-concept experiment, we utilized this method to profile cider aroma development throughout the fermentation process for the first time. The results yielded a very practical outcome for cider makers: a pre-screening method for determining cider quality through the detection of off-flavors early in the fermentation process. The aroma profile was found to be well established 72 h after fermentation commenced, with major esters varying by 18.6\% \&plusmn; 4.1\% thereafter and higher alcohols varying by just 12.3\% \&plusmn; 2.6\%. Lastly, we analyzed four mature ciders that were identically prepared, save for the yeast strain. Twenty-seven key VOCs were identified, off-flavors (4-ethylphenol and 4-ethyl-2-methoxyphenol) were detected, and odorants were quantified at desirable concentrations when compared to perception thresholds. VOCs varied considerably following fermentation with four novel strains of S. cerevisiae, evidencing the central importance of yeast strain to the finished cider aroma.},
  keywords = {aroma,cider,fermentation,gas chromatography,solid phase microextraction,yeast},
  file = {/Users/jake/Dropbox/Work/Zotero PDF Files/Bingman et al_2020_Monitoring Cider Aroma Development throughout the Fermentation Process by.pdf}
}

