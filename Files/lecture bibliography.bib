@article{utter2018,
  title = {Self-{{Perceived Cooking Skills}} in {{Emerging Adulthood Predict Better Dietary Behaviors}} and {{Intake}} 10 {{Years Later}}: {{A Longitudinal Study}}},
  shorttitle = {Self-{{Perceived Cooking Skills}} in {{Emerging Adulthood Predict Better Dietary Behaviors}} and {{Intake}} 10 {{Years Later}}},
  author = {Utter, Jennifer and Larson, Nicole and Laska, Melissa N. and Winkler, Megan and {Neumark-Sztainer}, Dianne},
  year = {2018},
  month = may,
  journal = {Journal of Nutrition Education and Behavior},
  volume = {50},
  number = {5},
  pages = {494--500},
  issn = {14994046},
  doi = {10.1016/j.jneb.2018.01.021},
  abstract = {Objective: To determine whether perceived cooking skills in emerging adulthood predicts better nutrition a decade later. Methods: Data were collected as part of the Project Eating and Activity in Teens and Young Adults longitudinal study. Participants reported on adequacy of cooking skills in 2002\textendash 2003 (age 18\textendash 23 years) and subsequently reported on nutrition-related outcomes in 2015\textendash 2016 (age 30\textendash 35 years) (n = 1,158). Separate regression models were used to examine associations between cooking skills at age 18\textendash 23 years and each subsequent outcome. Results: One fourth of participants described their cooking skills as very adequate at 18\textendash 23 years, with no statistically significant differences by sociodemographic characteristics. Reports of very adequate cooking skills at age 18\textendash 23 years predicted better nutrition-related outcomes 10 years later, such as more frequent preparation of meals including vegetables (P {$<$} .001) and less frequent fast food consumption (P {$<$} .001). Conclusions and Implications: Developing adequate cooking skills by emerging adulthood may have long-term benefits for nutrition over a decade later. Ongoing and new interventions to enhance cooking skills during adolescence and emerging adulthood are warranted but require strong evaluation designs that observe young people over a number of years.},
  langid = {english},
  keywords = {cooking,eating,longitudinal,nutrition},
  file = {/Users/jake/Dropbox/Work/Zotero PDF Files/Utter et al_2018_Self-Perceived Cooking Skills in Emerging Adulthood Predict Better Dietary.pdf}
}

@article{vabo2014,
  title = {The {{Relationship}} between {{Food Preferences}} and {{Food Choice}}: {{A Theoretical Discussion}}},
  author = {Vab{\o}, Mette and Hansen, H{\aa}vard},
  year = {2014},
  volume = {5},
  number = {7},
  pages = {13},
  abstract = {This paper discusses the relationship between food preferences and food choice. First, we aim to identify and point out what factors affect our food preferences and thereby try to explain the many reasons for differing food preferences. The underlying reasons might come from different sources, and this paper focuses on trying to fully grasp this complexity. We illustrate the variation of factors affecting our food preferences; biological/physiological, psychological and societal factors. Second, we aim to give a general overview over factors affecting food choice and the actual decision making process used by consumers. Third, we provide a brief discussion regarding the connection between food preferences and food choice. Finally, we conclude by stating that further insight is needed in areas such as cultural, historical and demographic factors. One solution to this could be to allow methodological influence from other disciplines such as social psychology.},
  langid = {english},
  file = {/Users/jake/Dropbox/Work/Zotero PDF Files/Vabø_Hansen_2014_The Relationship between Food Preferences and Food Choice.pdf}
}

