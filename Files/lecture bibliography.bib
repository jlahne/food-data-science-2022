@article{luc2020,
  title = {Nudging Consumers for Relevant Data Using {{Free JAR}} Profiling: {{An}} Application to Product Development},
  author = {Luc, Alexiane and L{\^e}, Sebastien and Philippe, Mathilde},
  year = {2020},
  journal = {Food Quality and Preference},
  volume = {79},
