@article{haugaard2016,
  title = {Objective Measures of Meal Variety Lacking Association with Consumers' Perception of Variety with Self-Selected Buffet Meals at Work},
  author = {Haugaard, Pernille and Brockhoff, Per B. and L{\"a}hteenm{\"a}ki, Liisa},
  year = {2016},
