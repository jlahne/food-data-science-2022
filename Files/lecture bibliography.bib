@article{lahne2014a,
  title = {``{{A}} Little Information Excites Us.'' {{Consumer}} Sensory Experience of {{Vermont}} Artisan Cheese as Active Practice},
  author = {Lahne, Jacob and Trubek, Amy B.},
  year = {2014},
